.expression {
  position: absolute;
  bottom: 0;
  width: 100%;
  background-color: #ffffff;
}
.expression .expression-tar {
  white-space: nowrap;
  overflow-x: auto;
  border-top: solid 1px #edecef;
  border-bottom: solid 1px #edecef;
  position: relative;
  background-color: transparent;
  padding: 5px 0;
}
.expression .expression-tar .iconfont {
  display: inline-block;
  font-size: 22px;
  padding: 5px 20px;
}
.expression .expression-tar .expression-tar-poasition {
  position: relative;
  z-index: 1;
}
.expression .expression-tar .expression-tar-back {
  width: calc(25px + 5px);
  height: calc(25px + 5px);
  background-color: #f0f0f0;
  position: absolute;
  top: 50%;
  -webkit-transform: translateY(-50%);
          transform: translateY(-50%);
  left: 3;
  z-index: 0;
  border-radius: 5px;
}
.expression .expression-tar::-webkit-scrollbar {
  display: none;
}
.expression .expression-swiper-content {
  position: relative;
}
.expression .expression-swiper-content .expression-swiper-empji-button-content {
  position: absolute;
  width: calc(((100% - 15rpx * 2 * 5) / 5) * 2);
  border: solid 1px #f0f0f0;
  height: calc(((100vw - 15rpx * 2 * 5) / 5));
  background-color: red;
  right: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  background-color: #f0f0f0;
  box-shadow: 20px 20px 50px #f5f5f5 inset;
}
.expression .expression-swiper-content .expression-swiper-empji-button-content .expression-button {
  display: inline-block;
  height: calc(((100vw - 15rpx * 2 * 5) / 5) / 2);
  line-height: calc(((100vw - 15rpx * 2 * 5) / 5) / 2);
  font-size: 14px;
}
.expression .expression-swiper-content .expression-swiper-empji-button-content .expression-button.send {
  background-color: #FBE836;
}
.expression .expression-swiper {
  background-color: #f0f0f0;
}
.expression .expression-swiper-item-text {
  overflow: auto;
  display: block;
}
.expression .expression-swiper-item-text .expression-swiper-list-empji {
  padding-bottom: calc(calc(((100vw - 15rpx * 2 * 5) / 5)) + 10px);
}
.expression .expression-swiper-item-text .expression-swiper-list-empji .expression-swiper-item-empji {
  display: inline-block;
  width: 20%;
  text-align: center;
  padding: 5px 0;
}
.expression .expression-swiper-item-images {
  display: flex;
  flex-wrap: wrap;
  overflow: auto;
}
.expression .expression-swiper-item-images .expression-swiper-item-img {
  width: calc((100% - 15rpx * 2 * 5) / 5);
  height: calc((100vw - 15rpx * 2 * 5) / 5);
  margin: 10px 15rpx;
  background-color: #ffffff;
}
.expression .expression-swiper-item-images .expression-swiper-item-img .expression-img-addImg {
  border: dashed 1px #000000;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 10px;
}
.expression .expression-swiper-item-images .expression-swiper-item-img .expression-img-addImg .iconfont.icon-add {
  font-size: calc((100vw - 15rpx * 2 * 5) / 5 * 0.5);
}
.expression .expression-swiper-item-images .expression-swiper-item-img .expression-img-swiper {
  width: 100%;
  height: 100%;
  display: inline-block;
}
.expression .popup-content {
  width: 100%;
  height: 100vh;
  overflow: auto;
  background-color: #f0f0f0;
  position: relative;
}
.expression .popup-content .content-expression {
  display: flex;
  flex-wrap: wrap;
}
.expression .popup-content .content-expression .content-expression-item {
  background-color: #ffffff;
  width: calc((100% - 8 * 5px) / 4);
  height: calc((100vw - 8 * 5px) / 4);
  margin: 5px;
  position: relative;
}
.expression .popup-content .content-expression .content-expression-item .expression-img-swiper {
  display: block;
  width: 100%;
  height: 100%;
}
.expression .popup-content .content-expression .content-expression-item .expression-u-checkbox {
  position: absolute;
  right: 0px;
  bottom: 0px;
  display: inline-block;
  width: 100%;
  height: 100%;
}
.expression .popup-content .content-expression .content-expression-item .expression-u-checkbox .expression-u-checkbox-offset {
  position: absolute;
  right: 5px;
  bottom: 5px;
}
.expression .popup-content .content-expression .content-expression-item.addImg {
  display: flex;
  justify-content: center;
  align-items: center;
}
.expression .popup-content .content-expression .content-expression-item.addImg .iconfont.icon-add {
  font-size: 30px;
}
.expression .popup-content .popup-content-delete {
  display: flex;
  align-items: center;
  flex-direction: row-reverse;
  padding-right: 20px;
  box-sizing: border-box;
  position: absolute;
  bottom: 0;
  background-color: #FFFFFF;
  width: 100%;
  height: 50px;
  color: #f9ce2f;
}

